	<entity model="greenArrow" name="g2"> <origin xyz="0.75 0.10 1.42" rpy="-1.5708 0 1.04"/> </entity>
	<entity model="greenArrow" name="g3"> <origin xyz="0.75 0.20 1.52" rpy="-2.09 0 1.5708"/> </entity>
	<entity model="greenArrow" name="g4"> <origin xyz="0.75 0.30 1.42" rpy="-1.5708 0.0 2.09"/> </entity>

</world> 
